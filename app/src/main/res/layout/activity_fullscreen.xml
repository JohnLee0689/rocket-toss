<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/classbg"
    tools:context="com.example.marissa.rocket_toss.FullscreenActivity">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->

    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->

    <Button
        android:text=" "
        android:layout_width="181dp"
        android:layout_height="75dp"
        android:id="@+id/button2"
        android:color="66FF0000"
        android:alpha="0.001"
        style="?android:attr/borderlessButtonStyle"
        tools:layout_editor_absoluteY="408dp"
        tools:layout_editor_absoluteX="112dp" />

    <TextView
        android:id="@+id/fullscreen_content"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:gravity="center"
        android:keepScreenOn="true"
        android:text="@string/dummy_content"
        android:textColor="#33b5e5"
        android:textSize="50sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="408dp"
        tools:layout_editor_absoluteX="0dp" />


    <LinearLayout
        android:id="@+id/fullscreen_content_controls"
        style="?metaButtonBarStyle"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@drawable/rockettoss1"
        android:orientation="vertical"
        tools:ignore="UselessParent"
        tools:layout_editor_absoluteY="409dp"
        tools:layout_editor_absoluteX="1dp">

        <Button
            android:id="@+id/button"
            style="?metaButtonBarButtonStyle"
            android:layout_width="160dp"
            android:layout_height="67dp"
            android:text="@string/dummy_button"
            android:layout_weight="0.55"
            android:layout_marginBottom="194dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="511dp"
            app:srcCompat="@drawable/classbg"
            android:id="@+id/imageView2"
            android:layout_weight="0.55"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp" />

    </LinearLayout>

    <ImageView
        android:layout_width="99dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/yellowshirt"
        tools:layout_editor_absoluteY="4dp"
        tools:layout_editor_absoluteX="269dp"
        android:id="@+id/imageView8" />

    <ImageView
        android:layout_width="87dp"
        android:layout_height="84dp"
        app:srcCompat="@drawable/redshirt"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="146dp"
        android:id="@+id/imageView4" />

    <ImageView
        android:layout_width="181dp"
        android:layout_height="75dp"
        app:srcCompat="@drawable/RocketsLogo_NoTextOL"
        tools:layout_editor_absoluteY="416dp"
        tools:layout_editor_absoluteX="112dp"
        android:id="@+id/imageView" />

    <ImageView
        android:layout_width="82dp"
        android:layout_height="88dp"
        app:srcCompat="@drawable/blueshirt"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="16dp"
        android:id="@+id/imageView3" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="0dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:layout_width="110dp"
        android:layout_height="99dp"
        app:srcCompat="@drawable/blueshirthand"
        tools:layout_editor_absoluteY="96dp"
        tools:layout_editor_absoluteX="132dp"
        android:id="@+id/imageView9" />

    <ImageView
        android:layout_width="91dp"
        android:layout_height="89dp"
        app:srcCompat="@drawable/yellowshirt"
        tools:layout_editor_absoluteY="96dp"
        tools:layout_editor_absoluteX="16dp"
        android:id="@+id/imageView10" />

    <ImageView
        android:layout_width="92dp"
        android:layout_height="102dp"
        app:srcCompat="@drawable/redshirt"
        tools:layout_editor_absoluteY="89dp"
        tools:layout_editor_absoluteX="276dp"
        android:id="@+id/imageView11" />

    <ImageView
        android:layout_width="113dp"
        android:layout_height="104dp"
        app:srcCompat="@drawable/redshirthand"
        tools:layout_editor_absoluteY="211dp"
        tools:layout_editor_absoluteX="129dp"
        android:id="@+id/imageView12" />

    <ImageView
        android:layout_width="104dp"
        android:layout_height="128dp"
        app:srcCompat="@drawable/yellowshirthand"
        tools:layout_editor_absoluteY="203dp"
        tools:layout_editor_absoluteX="9dp"
        android:id="@+id/imageView14" />

    <ImageView
        android:layout_width="99dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/blueshirt"
        tools:layout_editor_absoluteY="211dp"
        tools:layout_editor_absoluteX="264dp"
        android:id="@+id/imageView15" />


</android.support.constraint.ConstraintLayout>
